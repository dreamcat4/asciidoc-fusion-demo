    --lighter-0: brightness(120%);
    --lighter-1: brightness(140%);
    --lighter-2: brightness(160%);
    --lighter-3: brightness(180%);
    --lighter-4: brightness(200%);

    --darker-0: brightness(90%);
    --darker-1: brightness(75%);
    --darker-2: brightness(60%);
    --darker-3: brightness(40%);
    --darker-4: brightness(20%);

    --hue-0: hue-rotate(0deg);
    --hue-1: hue-rotate(72deg);
    --hue-2: hue-rotate(144deg);
    --hue-3: hue-rotate(216deg);
    --hue-4: hue-rotate(288deg);

    --invert-0: invert(15%);
    --invert-1: invert(35%);
    --invert-2: invert(65%);
    --invert-3: invert(85%);
    --invert-4: invert(100%);

    --sat-0: saturate(120%);
    --sat-1: saturate(140%);
    --sat-2: saturate(160%);
    --sat-3: saturate(180%);
    --sat-4: saturate(200%);

    --desat-0: saturate(80%);
    --desat-1: saturate(60%);
    --desat-2: saturate(40%);
    --desat-3: saturate(20%);
    --desat-4: saturate(0%);
  }
}

