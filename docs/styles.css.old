@supports (--css: variables) {
  :root {
    /* auto generated css variable mapping */
    /* ----------------------------------- */
    --fg-color-0: var(--color-0);
    --fg-color-1: var(--color-1);
    --fg-color-2: var(--color-2);
    --fg-color-3: var(--color-3);
    --fg-color-4: var(--color-4);
    --fg-color-5: var(--color-a);
    --fg-color-6: var(--color-b);
    --fg-color-7: var(--color-c);

    --bg-color-0: var(--color-5);
    --bg-filter-0: var(--filter-5);

    --bg-color-1: var(--color-6);
    --bg-color-2: var(--color-7);
    --bg-color-3: var(--color-8);
    --bg-color-4: var(--color-9);
    --bg-color-5: var(--color-d);
    --bg-color-6: var(--color-e);
    --bg-color-7: var(--color-f);

    --font-color-0: var(--fg-color-0);
    --font-color-1: var(--fg-color-1);
    --font-color-2: var(--fg-color-2);
    --font-color-3: var(--fg-color-3);
    --font-color-4: var(--fg-color-4);

    --hl-font-color: var(--font-color-2);
    --hl-bg-color: var(--bg-color-2);

    --bg-color: var(--bg-color-0);
    --bg-filter: var(--bg-filter-0);

    --bg-color-alt: var(--bg-color-5);
  }
}

