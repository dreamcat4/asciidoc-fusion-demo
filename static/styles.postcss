/*body { padding-top: 50px }*/
body>.navbar { -webkit-transition: background-color .3s ease-in; transition: background-color .3s ease-in }


#home { padding-top: 0 }
#home .navbar-brand { padding: 13.5px 15px 12.5px }
#home .navbar-brand>img { display: inline; margin: 0 10px; height: 100% }
#banner { min-height: 300px; border-bottom: none }
.table-of-contents { margin-top: 1em }
.page-header h1 { font-size: 4em }
.bs-docs-section { margin-top: 6em }
.bs-docs-section h1 { padding-top: 100px }
.bs-component { position: relative }
.bs-component .modal { position: relative; top: auto; right: auto; left: auto; bottom: auto; z-index: 1; display: block }
.bs-component .modal-dialog { width: 90% }
.bs-component .popover { position: relative; display: inline-block; width: 220px; margin: 20px }
#source-button { position: absolute; top: 0; right: 0; z-index: 100; font-weight: 700 }
.nav-tabs { margin-bottom: 15px }
.progress { margin-bottom: 10px }
footer { margin: 0em 0 }
footer li { float: left; margin-right: 1.5em; margin-bottom: 1.5em }
footer p { clear: left; margin-bottom: 0 }
.splash { padding: 9em 0 2em; background-color: #141d27; background-image: url(../img/bg.jpg); background-size: cover; background-attachment: fixed; color: #fff; text-align: center }
.splash .logo { width: 160px }
.splash h1 { font-size: 3em }
.splash #social { margin: 2em 0 }
.splash .alert { margin: 2em 0 }
.section-tout { padding: 4em 0 3em; border-bottom: 1px solid rgba(0, 0, 0, .05); background-color: #eaf1f1 }
.section-tout .fa { margin-right: .5em }
.section-tout p { margin-bottom: 3em }
.section-preview { padding: 4em 0 4em }
.section-preview .preview { margin-bottom: 4em; background-color: #eaf1f1 }
.section-preview .preview .image { position: relative }
.section-preview .preview .image:before { box-shadow: inset 0 0 0 1px rgba(0, 0, 0, .1); position: absolute; top: 0; left: 0; width: 100%; height: 100%; content: ""; pointer-events: none }
.section-preview .preview .options { padding: 1em 2em 2em; border: 1px solid rgba(0, 0, 0, .05); border-top: none; text-align: center }
.section-preview .preview .options p { margin-bottom: 2em }
.section-preview .dropdown-menu { text-align: left }
.section-preview .lead { margin-bottom: 2em }
@media (max-width:767px) {
	.section-preview .image img { width: 100% }
}
.sponsor #carbonads { max-width: 240px; margin: 0 auto }
.sponsor .carbon-text { display: block; margin-top: 1em; font-size: 12px }
.sponsor .carbon-poweredby { float: right; margin-top: 1em; font-size: 10px }
@media (max-width:767px) {
	.splash { padding-top: 4em }
	.splash .logo { width: 100px }
	.splash h1 { font-size: 2em }
	#banner { margin-bottom: 2em; text-align: center }
}
.box { margin-bottom: 30px; }


@supports not (--css: variables) {
  /* put here an error message on the screen */
  /* this theme does not function in older web browsers */
}

@supports (--css: variables) {
  :root {
    /* --navbar-collapse-width: 398px; */
    /* --navbar-collapse-width: 568px; */
    /* --navbar-collapse-width: #`$`###hide_navbar; */




    --hide-pagenav: $hide_pagenav;
    --hide-sidebar: $hide_sidebar;





    /* --navbar-height: 30px; */
    /* --navbar-height-negative: -30px; */

    --navbar-height: $navbar_height;
    --navbar-height-negative: -$navbar_height;

    /* --navbar-height: 40px; */
    /* --navbar-height-negative: -40px; */

    /* --navbar-height: 45px; */
    /* --navbar-height-negative: -45px; */

    /* --navbar-height: 50px; */
    /* --navbar-height-negative: -50px; */


    /* --navbar-height-negative: calc(0 - var(--navbar-height)); */

    /* auto generated css variable mapping */
    /* uses the css color() funtion https://drafts.csswg.org/css-color/#modifying-colors */
    /* ----------------------------------- */
    --fg-color-0: $fgcolor0;
    --fg-color-1: $fgcolor1;
    --bg-color-0: $bgcolor0;


    @if $color_generation_mode == light {

      --fg-color-2: color($fgcolor0 tint(30%));
      --fg-color-3: color($fgcolor0 blend(#efd6ac 50%));
      /* --fg-color-3: color(#d64714 blend(#efd6ac 60%)); */
      --fg-color-4: color($fgcolor1 saturation(130%));
      --fg-color-5: color($bgcolor0 lightness(50%));
      --fg-color-6: color($bgcolor0 shade(50%));
      --fg-color-7: color($fgcolor1 lightness(35%));

      --bg-color-1: color($bgcolor0 tint(17%));

      --bg-color-2: color($bgcolor0 tint(37%));
      --bg-color-3: color($bgcolor0 shade(10%));

      --bg-color-4: color($bgcolor0 saturation(75%));
      --bg-color-5: color($bgcolor0 whiteness(50%));

      --bg-color-6: color($bgcolor0 blackness(13%));
      --bg-color-7: color($bgcolor0 saturation(50%));

      /* --sidebar-color: linear-gradient(to right, var(--bg-color-7), var(--bg-color)); */
      /* --pagenav-color: linear-gradient(to right, var(--bg-color-7), var(--bg-color)); */

      --sidebar-color: linear-gradient(to right, var(--bg-color), var(--bg-color-7));
      --pagenav-color: linear-gradient(to right, var(--bg-color), var(--bg-color-7));

      --toggle-sidebar: var(--bg-color-4);
      --toggle-pagenav: var(--bg-color-4);
    }

    @else if $color_generation_mode == dark {

      --fg-color-2: color($fgcolor0 tint(70%));
      --fg-color-3: color($fgcolor0 blend(#efd6ac 50%));
      /* --fg-color-3: color(#d64714 blend(#efd6ac 60%)); */
      --fg-color-4: color($fgcolor1 saturation(30%));
      --fg-color-5: color($bgcolor0 lightness(90%));
      /* --fg-color-6: color(var(--fg-color-2) saturation(0%)); */
      --fg-color-6: color($fgcolor1 saturation(13%));
      --fg-color-7: color($fgcolor1 shade(35%));

      --bg-color-1: color($bgcolor0 tint(5%));

      --bg-color-2: color($bgcolor0 tint(10%));
      --bg-color-3: color($bgcolor0 lightness(10%));

      --bg-color-4: color($bgcolor0 saturation(50%));
      --bg-color-5: color($bgcolor0 blackness(50%));

      --bg-color-6: color($bgcolor0 whiteness(13%));
      --bg-color-7: color($bgcolor0 saturation(10%));

      --sidebar-color: $bgcolor0;
      --pagenav-color: $bgcolor0;

      /* --toggle-sidebar: var(--bg-color-0); */
      /* --toggle-pagenav: var(--bg-color-0); */
      --toggle-sidebar: var(--bg-color-3);
      --toggle-pagenav: var(--bg-color-3);
      /* --toggle-sidebar: var(--bg-color-4); */
      /* --toggle-pagenav: var(--bg-color-4); */
    }

  }
}

@supports (--css: variables) {
  :root {
    --font-size-0: 16px;
    --font-size-1: 14px;
    --font-size-2: 12px;

    /* descriptive */
    --bg-color-light: var(--bg-color-1);
    --bg-color-dark: var(--bg-color-2);

    --bg-color: var(--bg-color-0);
    --bg-color-alt: var(--bg-color-2);

    --fg-color: var(--fg-color-0);
    --fg-color-alt: var(--fg-color-1);
    --fg-color-greyed: var(--fg-color-2);

    --hl-bg-color: var(--bg-color-1);
    --hl-font-color: var(--fg-color-4);

    --hv-bg-color: var(--bg-color-2);
    --hv-font-color: var(--fg-color-4);

    --navbar-color: var(--bg-color-1);
    --navbar-active-color: var(--bg-color-0);
    --navbar-hv-bg-color: var(--hv-bg-color);


    /* show and hide panel animation, for sidebar and pagenav */
    --panel-transition-time: 1s;


    --title-color: var(--fg-color-2);
    --hr-color: var(--bg-color-3);

    --text-color: var(--fg-color-6);
    --link-color: var(--fg-color-1);
    /* --link-bg-color: var(--bg-color-6); */

    --active-font-color: var(--fg-color-7);


    --table-head-fg-color: var(--fg-color-2);
/*    --table-head-bg-color: -webkit-linear-gradient(top, var(--bg-color-7), var(--bg-color-6)); */
    --table-head-bg-color: -webkit-linear-gradient(top, var(--bg-color-5), var(--bg-color-4));
    --table-row-color-odd: var(--bg-color-7);
    --table-row-color-even: var(--bg-color-6);

/*    --table-border: 1px solid var(--fg-color-2); */
    --table-border: 1px solid var(--bg-color-3);


    /* individual css variable overrides */
    /* --------------------------------- */
    /* regular links */
    /* --link-color: #ff00ff; */
    /* --link-bg-color: #ff00ff; */

    /* highlighted links */
    /* --link-hl-font-color: #ff00ff; */
    /* --link-hl-bg-color: #ff00ff; */

    /* hovering over links */
    /* --link-hv-font-color: #ff00ff; */
    /* --link-hv-bg-color: #ff00ff; */

    /* sidebar menu, and page navigation menu */
    /* --menu-font-family: Courier New, Monospace; */
    /* --menu-font-size: 16px; */
    /* --menu-indent: 0.5em; */

    --code-padding: 20px;
    --code-bg-color: var(--bg-color-1);
    --code-font-color: var(--fg-color-0);
    --code-font-size: var(--font-size-0);

  --inline-code-font-color: var(--fg-color-1);
  --inline-code-bg-color: var(--bg-color-1);
  /* --inline-code-border: 1px solid; */
  --inline-code-border: 0px;


  --sidebar-block-bg-color: var(--bg-color-1);
  --sidebar-block-font-size: var(--font-size-0);
  --sidebar-block-font-color: var(--text-color);
  --sidebar-block-font-title-color: var(--text-color);


  --blockquote-bg-color: var(--bg-color-1);
  --blockquote-font-color: var(--text-color);
  --blockquote-font-size: var(--font-size-0);

  /* asciidoc blockquotes */
  --quoteblock-citation-color: var(--text-color);
  --quoteblock-attribution-color: var(--fg-color-7);
  --quoteblock-quote-size: 2.75em;
  --quoteblock-quote-height-offset: 0.1em;
  --quoteblock-line-height: 1em;
  --quoteblock-indent: 30px;
  --quoteblock-quote-indent: 10px;
  
  /* asciidoc admonition note blocks */
  --adblock-bg-color: var(--bg-color-1);
  --adblock-text-color: var(--text-color);
  // --adblock-font-size: var(--font-size-0);

  /* regular icons */
  --adblock-icon-color: var(--fg-color-2);
  --adblock-icon-color-note: var(--fg-color-2);
  --adblock-icon-color-tip: var(--fg-color-2);

  /* warning icons */
  --adblock-icon-color-important: var(--fg-color-4);
  --adblock-icon-color-warning: var(--fg-color-1);
  --adblock-icon-color-caution: var(--fg-color-7);

  --adblock-icon-shadow: 1px 1px 2px rgba(0, 0, 0, 0.5);
  /* --adblock-icon-shadow: none; */
  }
}

