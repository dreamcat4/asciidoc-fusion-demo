/* navbar height */
$navbar_height: 35px;

/* viewport sizes */


$small_viewport:  368px;
$medium_viewport: 568px;
$large_viewport:  768px;

@custom-media --small-viewport  (max-width: $medium_viewport);
@custom-media --medium-viewport (max-width: $large_viewport);
@custom-media --large-viewport  (min-width: $large_viewport);


/* $hide_pagenav: 1024px; */
/* $hide_sidebar:  768px; */

$hide_pagenav:  960px;
$hide_sidebar: 1280px;

/* $hide_navbar:   768px; */

